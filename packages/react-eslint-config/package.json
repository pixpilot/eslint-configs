{
  "name": "@pixpilot/react-eslint-config",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@10.12.2",
  "author": "pixpilot",
  "license": "MIT",
  "keywords": [],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublishOnly": "pnpm run clean && pnpm run build",
    "clean": "rimraf dist",
    "build": "pnpm run clean && rollup -c",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit",
    "lint": "eslint",
    "format": "prettier --check . --ignore-path ../../.gitignore --ignore-path ../../.prettierignore"
  },
  "dependencies": {
    "@eslint-react/eslint-plugin": "catalog:eslint",
    "@fastify/deepmerge": "catalog:prod",
    "@pixpilot/eslint-config": "catalog:eslint",
    "eslint-plugin-jsx-a11y": "catalog:eslint",
    "eslint-plugin-react": "catalog:eslint",
    "eslint-plugin-react-hooks": "catalog:eslint",
    "eslint-plugin-react-refresh": "catalog:eslint"
  },
  "devDependencies": {
    "@internal/eslint-config": "workspace:*",
    "@internal/prettier-config": "workspace:*",
    "@internal/rollup-config": "workspace:*",
    "@internal/semantic-release": "workspace:*",
    "@internal/tsconfig": "workspace:*",
    "@internal/types": "workspace:*",
    "@internal/vitest-config": "workspace:*",
    "@pixpilot/dev-config": "catalog:dev",
    "@types/node": "catalog:dev",
    "eslint": "catalog:eslint",
    "rollup": "catalog:dev"
  }
}

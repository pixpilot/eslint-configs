{
  "name": "@pixpilot/eslint-config-base",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@10.14.0",
  "author": "m.doaie <m.doaie@hotmail.com> (https://github.com/ccpu)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git/eslint-config-base.git"
  },
  "keywords": [],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "workspace-development-source": "./src/index.ts"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "pnpm run clean && pnpm run build",
    "clean": "rimraf dist",
    "build": "pnpm run clean && rollup -c",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint",
    "format": "prettier --check . --ignore-path ../../.gitignore --ignore-path ../../.prettierignore"
  },
  "dependencies": {
    "@internal/utils": "workspace:*"
  },
  "devDependencies": {
    "@internal/eslint-config": "workspace:*",
    "@internal/prettier-config": "workspace:*",
    "@internal/rollup-config": "workspace:*",
    "@internal/tsconfig": "workspace:*",
    "@internal/types": "workspace:*",
    "@internal/vitest-config": "workspace:*",
    "@pixpilot/dev-config": "catalog:dev",
    "@types/node": "catalog:dev",
    "rollup": "catalog:dev"
  }
}
